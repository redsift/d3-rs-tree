{
  "name": "@redsift/d3-rs-tree",
  "version": "0.2.0",
  "description": "Generates tree charts using D3v4.",
  "keywords": [
    "tree",
    "redsift",
    "d3",
    "chart"
  ],
  "homepage": "https://github.com/redsift/d3-rs-tree",
  "license": "MIT",
  "author": {
    "name": "Rahul Powar",
    "url": "https://medium.com/@rahulpowar"
  },
  "main": "distribution/d3-rs-tree.umd-es2015.js",
  "browser": "distribution/d3-rs-tree.umd-es2015.min.js",
  "jsnext:main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/redsift/d3-rs-tree.git"
  },
  "scripts": {
    "serve": "gulp -o d3-rs-tree -g d3-selection serve",
    "prepublish": "npm run clean; npm run build",
    "build": "npm run clean; node ./node_modules/@redsift/rollup-bundler/src/index.js bundle.config.js",
    "clean": "rm -rf dist",
    "pretest": "npm outdated || true && npm run build",
    "test": "tape 'test/**/*-test.js'",
    "posttest": "eslint index.js src"
  },
  "dependencies": {
    "@redsift/d3-rs-svg": "^0.7.0",
    "@redsift/d3-rs-theme": "^0.6.0",
    "@redsift/d3-rs-tip": "^0.10.0",
    "d3-array": "^1.2.1",
    "d3-hierarchy": "^1.1.5",
    "d3-scale": "^1.0.7",
    "d3-selection": "^1.2.0",
    "d3-shape": "^1.2.0"
  },
  "devDependencies": {
    "@redsift/rollup-bundler": "^2.0.2",
    "@redsift/tape-reel": "~0.1.1",
    "browser-sync": "^2.14.0",
    "del": "^2.2.2",
    "eslint": "^3.3.1",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "gulp-util": "^3.0.8",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "yargs": "^5.0.0"
  },
  "overrides": {
    "lodash": "4.17.21",
    "lodash.template": "4.5.0",
    "chokidar": "3.5.3",
    "glob-parent": "6.0.2"
  }
}
